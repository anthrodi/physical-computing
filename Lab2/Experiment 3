int ledPin = 10; //LED connected to digital pin 10

void setup() {                
  
  pinMode(ledPin, OUTPUT);     
}

void loop() {
  analogWrite(ledPin, 50);   
  delay(2000);  // 
  analogWrite(ledPin, 200);    
  delay(2000);               
}
